<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

	<fragment android:id="@+id/rents_map"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          class="com.personal.rents.fragments.RentsMapFragment"/>

	<ImageButton android:id="@+id/share_app_btn"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginTop="58dp"
	    android:layout_marginRight="10dp"
	    android:layout_gravity="right"
	    android:background="@drawable/custom_share_app_btn"
	    android:contentDescription="@string/share_app_img_desc" />

</FrameLayout>
